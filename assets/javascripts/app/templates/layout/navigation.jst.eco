<ul class="navigation">
  <% for value in @values: %>
    <li class="<%= "active" if value.link == @values.active %>">
      <% if value.link == @values.active: %>
        <span><%= value.name %></span>
      <% else: %>
        <%= @linkTo value.name, value.link %>
      <% end %>
    </li>
  <% end %>
  <li class="search">
    <input type="text" name="query" value="" class="" placeholder="Search" />
  </li>
</ul>
