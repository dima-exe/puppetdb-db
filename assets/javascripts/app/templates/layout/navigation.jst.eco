<ul class="navigation">
  <% for value in @values: %>
    <li class="<%= "active" if value.link == @values.active %>">
      <%= @linkTo value.name, value.link %>
    </li>
  <% end %>
  <li class="search">
    <input type="text" name="query" value="" class="input-small search-query" placeholder="Search" />
  </li>
</ul>
