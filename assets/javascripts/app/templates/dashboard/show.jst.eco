<div id="node-reports-summary-chart"></div>

<table class="table table-nodes">
  <thead>
    <th>Name</th>
    <th>Last report</th>
  </thead>
  <% for node in @nodes.models: %>
    <tr>
      <td>
        <%= @linkTo node %>
      </td>
      <td><%= node.reportAt() %></td>
    </tr>
  <% end %>
</table>
